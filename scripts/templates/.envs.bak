# Set custom environment variables for configs.

# Your machine name.
export MACHINE_NAME=""

# Network card Ids. Use '$ ifconfig' to check network settings.
export WIRELESS_NETWORK=""
export WIRED_NETWORK=""

# Monitor name. Use '$ arandr' to check active monitors.
export MONITOR_NAME=""

# Token used for polybar module github
export TOKEN_GITHUB=""

# Main text editor
export MAIN_EDITOR="code"

# Main terminal text editor
export MAIN_TERM_EDITOR="vim"

# Nscript variables
export NSCRIPT_SCRIPT_DIR=""
export NSCRIPT_EXECUTABLE_DIR=""
export NSCRIPT_EDITOR=""
